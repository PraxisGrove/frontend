# 应用配置
NEXT_PUBLIC_APP_NAME=PraxisGrove
NEXT_PUBLIC_APP_VERSION=0.1.0
NEXT_PUBLIC_APP_ENV=development

# API配置
NEXT_PUBLIC_API_BASE_URL=http://localhost:8000/api
NEXT_PUBLIC_API_TIMEOUT=10000

# 认证配置
NEXT_PUBLIC_JWT_SECRET=your-jwt-secret-key
NEXT_PUBLIC_REFRESH_TOKEN_EXPIRY=7d

# 3D渲染配置
NEXT_PUBLIC_ENABLE_3D_FEATURES=true
NEXT_PUBLIC_MAX_3D_NODES=1000

# 开发工具配置
NEXT_PUBLIC_ENABLE_DEVTOOLS=true
NEXT_PUBLIC_LOG_LEVEL=debug

# 第三方服务配置
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
# NEXT_PUBLIC_VERCEL_ANALYTICS_ID=

# 日志系统配置
LOG_LEVEL=info
LOGSTASH_HOST=localhost
LOGSTASH_PORT=5000
ELASTICSEARCH_URL=http://localhost:9200
# ELASTICSEARCH_USERNAME=
# ELASTICSEARCH_PASSWORD=

# CI/CD 配置 (GitHub Secrets)
# VERCEL_TOKEN=your_vercel_token
# VERCEL_ORG_ID=your_vercel_org_id
# VERCEL_PROJECT_ID=your_vercel_project_id
# SNYK_TOKEN=your_snyk_token
# LHCI_GITHUB_APP_TOKEN=your_lighthouse_github_app_token
